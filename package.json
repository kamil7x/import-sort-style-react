  "description": "An import-sort style that groups and sorts by module respecting React modules",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "watch:build": "tsc -p src -w",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive test",
    "watch:test": "mocha -w --compilers ts:ts-node/register,tsx:ts-node/register --recursive test",
    "format": "prettier --write src/*.ts"
  },
  "author": "Michael Manukyan <michael@teamable.com> (https://github.com/mike1808)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:kamil7x/import-sort-style-react.git"
  },
  "homepage": "https://github.com/teamable-software/import-sort-style-react#readme",
  "bugs": {
    "url": "https://github.com/teamable-software/import-sort-style-react/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.31",
    "chai": "^4.0.2",
    "import-sort-parser": "^4.2.0",
    "import-sort-style": "^4.2.0",
    "mocha": "^3.4.2",
    "prettier": "^1.15.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.4.3",
    "typescript": "^2.6.1"
  },
  "dependencies": {}
}
